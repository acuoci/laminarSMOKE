FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      zMix;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    fuelInlet
    {
        type            fixedValue;
        value           uniform 1.;
    }
    coflowInlet
    {
        type            fixedValue;
        value           uniform 0;
    }
    burnerWall
    {
        type            zeroGradient;
    }
    outlet
    {
        type            inletOutlet;
        inletValue      uniform 0.;
        value           uniform 0.;
    }
    leftSide
    {
        type            zeroGradient;
    }
    axis
    {
	type		empty;
    }
    front
    {
        type            wedge;
    }
    back
    {
        type            wedge;
    }
}

